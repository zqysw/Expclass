        android:icon="@drawable/syb"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.expclass.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.expclass.activity.PeopleActivity"
            android:label="@string/app_name" 
            android:theme="@style/AppTheme">  
        </activity>
        
         <activity
            android:name="com.expclass.activity.ProjectListActivity"
            android:label="@string/app_name" 
            android:theme="@style/AppTheme">  
        </activity>
        
         <activity
            android:name="com.expclass.activity.StudentInfoActivity"
            android:label="@string/app_name" 
            android:theme="@style/AppTheme">  
        </activity>
        <activity
            android:name="com.expclass.activity.FilpperImgActivity"
            android:label="@string/app_name" 
            android:theme="@style/AppTheme">  
        </activity>
        
        <activity
            android:name="com.expclass.activity.ProjectItemActivity"
            android:label="@string/app_name" 
            android:theme="@style/AppTheme">  
        </activity>
    </application>

</manifest>
